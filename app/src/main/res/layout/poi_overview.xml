<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".poi_overview">>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/overview_textviews_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/overview_type_amenity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Amenity"
                android:drawableLeft="@drawable/amenity_or_shop"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/overview_type_shop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/amenity_or_shop"
                android:text="Not a shop"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:paddingStart="10dp"
                android:textSize="20sp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/overview_address"
                android:text="Address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/address_icon"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:paddingStart="10dp"
                android:textSize="20sp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/overview_opening_hours"
                android:text="Opening hours"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/opening_hours_icon"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/overview_phone_number"
                android:text="Phone number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/call_phone_icon"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/overview_website"
                android:text="Website"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/website_icon"
                android:drawablePadding="10dp"
                android:padding="10dp"
                android:textSize="20sp" />

